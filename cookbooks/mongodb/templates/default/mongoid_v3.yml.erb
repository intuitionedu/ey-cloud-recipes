<%= @environment %>:
  sessions:
    default:
      hosts:
      <% @hosts.each do |host, port| -%>
        - <%= host %>:<%= port %>
      <% end -%> 
      database: <%=@mongo_replsetname%>
  # Mongoid specific configuration options
  options:
    identity_map_enabled: true
    raise_not_found_error: false
